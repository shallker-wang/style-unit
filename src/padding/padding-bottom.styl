.padding-bottom-auto
  padding-bottom auto

.padding-bottom-inherit
  padding-bottom inherit

padding-bottom-pixel(range)
  for i in range
    .{'padding-bottom-'+i}
      padding-bottom i * 1px

padding-bottom-pixel(0..100)

padding-bottom-percentage(range)
  for i in range
    .{'padding-bottom-'+i+'p'}
      padding-bottom i * 1%

